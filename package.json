{
  "name": "nederlandse-fietsknooppunten-tracker",
  "version": "1.0.0",
  "description": "Track your Dutch cycling node routes with real OpenStreetMap data",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "npm run build && nodemon dist/server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "download:nodes": "node dist/scripts/download-all-nodes.js",
    "download:routes": "node dist/scripts/download-all-nodes.js routes",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t fietsrouteapp .",
    "docker:run": "docker run -p 3000:3000 fietsrouteapp",
    "docker:compose": "docker-compose up --build",
    "docker:compose:prod": "docker-compose up -d --build"
  },
  "keywords": [
    "cycling",
    "netherlands",
    "openstreetmap",
    "fietsknooppunten",
    "routes",
    "tracking"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
